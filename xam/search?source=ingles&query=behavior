                             var brosersAndVersions = navigator.userAgent.match(/(\w+)\/([0-9.]+)/g);
                             var browsers = {};
                             for (var i = 0; i < brosersAndVersions.length; ++i) {
                                 var arr = brosersAndVersions[i].split("/");
                                 browsers[arr[0]] = parseFloat(arr[1]);
                             }

                             var offset = function(el) {
                                 var rect = el.getBoundingClientRect(), scrollLeft = window.pageXOffset || document.documentElement.scrollLeft, scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                                 return { top: rect.top + scrollTop, left: rect.left + scrollLeft };
                             };

                             var main = window.getComputedStyle(document.getElementById('mainlayout'));
                             var outer_div = offset(document.getElementById('outer_div'));
                             var mainPosition = offset(document.getElementById('mainlayout'));
                             var scrollListener = function(main, element, side) {
                                 var stickyPos = document.getElementById(element), offsetDiv = offset(stickyPos);
                                 stickyPos.style.width = main;
                                 window.addEventListener('scroll', function() {
                                     var topDistance = window.scrollY;
                                     if ($("#body").hasClass("parallax")) { topDistance = Math.abs(offset(document.getElementById('mainlayout')).top); }

                                     if (topDistance >= offsetDiv.top) {
                                         stickyPos.style.position = 'fixed';
                                         if ($("#body").hasClass("parallax")) {
                                             if (browsers["Safari"] >= 601) { stickyPos.style.top = '0px';
                                             } else { stickyPos.style.top = topDistance + 'px'; }
                                             stickyPos.style[side] = '0px';
                                         } else { stickyPos.style[side] = (outer_div.left + mainPosition.left) / 2 + 'px'; }
                                     } else {
                                         stickyPos.style.position = 'relative';
                                         stickyPos.style.top = '0px';
                                         stickyPos.style[side] = '0px';
                                     }
                                 }, true);
                             };

                             snhb.queue.push(function() {
                                 document.getElementById('snhb-right_side_top-0').style.width = main.getPropertyValue('padding-right');
                                 document.getElementById('snhb-left_side_top-0').style.width = main.getPropertyValue('padding-left');
                                 googletag.cmd.push(function() {
                                     googletag.pubads().addEventListener('slotRenderEnded', function() {
                                         document.getElementById('snigel_ads_right').style.top = ((window.innerWidth > 767 && window.innerWidth < 896) ? 108 : 0) + 'px';
                                         scrollListener(main.getPropertyValue('padding-left'), 'snhb-left_side_bottom_sticky-0', 'left');
                                         scrollListener(main.getPropertyValue('padding-right'), 'snhb-right_side_bottom_sticky-0', 'right');
                                     });
                                 });
                             });
